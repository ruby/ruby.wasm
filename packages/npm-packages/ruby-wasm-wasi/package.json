    "@types/jest": "^29.1.2",
    "@wasmer/wasi": "^0.12.0",
    "@wasmer/wasmfs": "^0.12.0",
    "babel-jest": "^29.1.2",
    "jest": "^29.1.2",
    "prettier": "^2.5.1",
